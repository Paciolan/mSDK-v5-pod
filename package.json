{
  "name": "msdk-v5",
  "version": "5.0.14",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "APP_ENV=development react-native start --reset-cache",
    "start:prod": "APP_ENV=production npx react-native start --reset-cache",
    "test": "jest",
    "ci:bundle:ios": "APP_ENV=production react-native bundle --reset-cache --dev false --entry-file sdk.js --bundle-output ./sdk-pod/PaciolanSDK/Assets/PaciolanSDK.js --assets-dest ./sdk-pod/PaciolanSDK/Assets",
    "test:coverage": "jest --coverage",
    "preci:bundle:android": "rm -rf ./sdk-gradle/mobilesdk/src/main/assets/index.android.bundle",
    "ci:bundle:android": "APP_ENV=production react-native bundle --reset-cache --platform android --dev false --entry-file sdk.js --bundle-output ./sdk-gradle/mobilesdk/src/main/assets/index.android.bundle --assets-dest ./sdk-gradle/mobilesdk/src/main/res/",
    "build:ios:qa:browserstack": "FASTLANE_OPT_OUT_USAGE=YES fastlane ios build",
    "build:android:qa:browserstack": "FASTLANE_OPT_OUT_USAGE=YES fastlane android build",
    "build:ios:release:browserstack": "FASTLANE_OPT_OUT_USAGE=YES fastlane ios release",
    "build:android:release:browserstack": "FASTLANE_OPT_OUT_USAGE=YES fastlane android build",
    "build:android:example": "FASTLANE_OPT_OUT_USAGE=YES fastlane android example",
    "build:ios:example1": "FASTLANE_OPT_OUT_USAGE=YES fastlane ios example1",
    "build:ios:example2": "FASTLANE_OPT_OUT_USAGE=YES fastlane ios example2",
    "ci:version": "node scripts/version.js",
    "wipe": "watchman watch-del-all && watchman watch-del-all && rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && rm -rf $TMPDIR/react-* && rm -rf android/.gradle && rm -rf android/build && rm -rf android/app/build && rm -rf ios/build && rm -rf ./build && rm -rf node_modules && npm cache verify",
    "wipe:install": "npm run wipe && npm install",
    "prepare": "husky",
    "generate-automation-tests-yaml": "ts-node --project tsconfig.scripts.json generateIntegrationYaml.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@react-native-community/cli": "^14.1.1",
    "@react-native-community/push-notification-ios": "^1.11.0",
    "@react-native-firebase/app": "^19.1.0",
    "@react-native-firebase/messaging": "^19.1.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@sentry/react-native": "5.31.0",
    "graphql": "^16.8.1",
    "npm": "^10.2.5",
    "react": "18.3.1",
    "react-native": "0.75.3",
    "react-native-action-sheet": "^2.2.0",
    "react-native-code-push": "^8.2.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-loggly-jslogger": "^2.2.3",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-select-contact": "^1.6.3",
    "react-native-wallet-manager": "^0.0.7",
    "react-native-webview": "^13.12.2",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-expire": "^0.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/plugin-transform-private-methods": "^7.25.4",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.6",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "@types/redux-mock-store": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-react-native": "^18.0.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.2.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.1.1",
    "react-native-codegen": "^0.70.7",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "redux-mock-store": "^1.5.4",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "babel": {
    "presets": [
      "module:metro-react-native-babel-preset"
    ]
  },
  "engines": {
    "node": ">=20"
  }
}
